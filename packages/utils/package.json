{
  "name": "@erm/utils",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm exec tsc -p tsconfig.json",
    "lint": "pnpm exec eslint -c ../../eslint.config.ts src --ext .ts",
    "typecheck": "pnpm exec tsc --noEmit -p tsconfig.json",
    "test": "node -e \"console.log('utils: no tests')\"",
    "ci:fast": "pnpm run build && pnpm run typecheck && pnpm run lint && pnpm run test",
    "ci:full": "pnpm run ci:fast"
  },
  "dependencies": {
    "shared": "workspace:*"
  }
}
