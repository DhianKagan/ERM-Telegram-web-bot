+const normalizeCoordinate = (value: unknown): number | undefined => {
+  const numeric =
+    typeof value === 'string'
+      ? Number.parseFloat(value)
+      : typeof value === 'number'
+        ? value
+        : Number.NaN;
+  return Number.isFinite(numeric) ? numeric : undefined;
