{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "predev": "node ../../scripts/ensure-fonts.mjs",
    "prebuild": "node ../../scripts/ensure-fonts.mjs",
    "dev": "vite",
    "build": "pnpm exec cross-env TAILWIND_DISABLE_ESM_LOADER=1 vite build",
    "build:dist": "pnpm run prebuild && pnpm exec vite build --outDir dist && node ../../scripts/prepare_size_artifacts.mjs",
    "lint": "pnpm exec eslint .",
    "preview": "vite preview",
    "format": "pnpm exec prettier --config ../../prettier.config.cjs \"src/**/*.{js,jsx,ts,tsx}\"",
    "shadcn": "shadcn",
    "bundle-report": "ANALYZE=true vite build",
    "test:types": "pnpm exec tsc --noEmit --lib ESNext,DOM --types node src/types/task.test.ts",
    "lint:lpt": "pnpm exec eslint -c ../../eslint.lpt.config.ts --ignore-pattern postcss.config.cjs --rule \"@typescript-eslint/no-unused-expressions: off\" --rule \"react-refresh/only-export-components: off\" ."
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^41.4.2",
    "@ckeditor/ckeditor5-react": "^6.2.0",
    "@hello-pangea/dnd": "^18.0.1",
