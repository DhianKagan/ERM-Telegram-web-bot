{
  "name": "worker",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "pnpm exec ts-node src/index.ts",
    "build": "pnpm exec tsc -b",
    "start": "node dist/index.js",
    "lint": "pnpm exec eslint -c ../../eslint.config.ts src",
    "typecheck": "pnpm exec tsc --noEmit",
    "test": "node -e \"console.log('worker: no tests')\"",
    "ci:fast": "pnpm run build && pnpm run typecheck && pnpm run lint && pnpm run test",
    "ci:full": "pnpm run ci:fast"
  },
  "dependencies": {
    "bullmq": "^5.31.1",
    "dotenv": "^16.6.1",
    "ioredis": "^5.4.1",
    "pino": "^9.9.4",
    "shared": "workspace:*",
    "mongodb": "^5.7.0"
  }
}
